@model ProfileViewModel
@{
    var imageArr = Model.User.Images.SingleOrDefault(el => el.ImageNumber == Model.User.SocNetItems.CurrentImage).Image;

    var strImage = ImageMethods.GetStringFromByteArr(imageArr);

    var format = ImageMethods.GetFormat(imageArr);

    var ava = new PhotoFormat
    {
        Format = format,
        Image = strImage
    };
}
@inject IUser userService;
